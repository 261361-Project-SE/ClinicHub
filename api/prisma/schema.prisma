generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Status {
  Pending
  Confirmed
  Canceled
  Completed
}

model Appointments {
  id                   Int      @id @default(autoincrement())
  eventId              String
  firstname            String
  lastname             String
  phone_number         String
  symptom              String
  appointment_dateTime String
  status               String
  createdAt            DateTime @default(now())
  updatedAt            DateTime @updatedAt
}
